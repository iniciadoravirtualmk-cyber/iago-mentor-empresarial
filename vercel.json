{
  "rewrites": [
    {
      "source": "/_next/:path*",
      "destination": "https://aura-sis.vercel.app/_next/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "https://aura-sis.vercel.app/api/:path*"
    },
    {
      "source": "/aura/:path*",
      "destination": "https://aura-sis.vercel.app/aura/:path*"
    }
  ]
}
